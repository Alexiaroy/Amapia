<style>
#carouselExampleCaptions {
	height: 400px;
}

.carousel-text {
	width: 550px;
}

.carousel-control-prev, .carousel-control-next {
	width: auto;
	height: auto;
	background: none;
	border: none;
}

.carousel-control-prev-icon, .carousel-control-next-icon {
	background-size: 100%;
}

.cards-container .product-card {
	width: 14rem;
	margin-right: 15px;
}

/* Sidebar styles */
.sidebar {
	width: 250px;
	height: 100%;
	background-color: #333;
	color: white;
	overflow-x: hidden;
	position: absolute;
	transform: translateX(-250px);
	transition: transform 0.3s ease;
}

.sidebar.active {
	transform: translateX(0);
}

.sidebar ul {
	list-style: none;
	padding: 0;
	margin: 0;
}

.sidebar ul li {
	padding: 15px;
	text-align: center;
}

.sidebar ul li a {
	color: white;
	text-decoration: none;
}

.sidebar ul li a:hover {
	background-color: #575757;
}

/* Main content styles */
.main-content {
	flex-grow: 1;
	padding: 15px;
	margin-left: 0;
	transition: margin-left 0.3s ease;
	position: relative;
	z-index: 1;
}

.main-content.sidebar-active {
	margin-left: 250px;
}

/* Toggle button */
.toggle-btn-container {
	margin-bottom: 15px;
	position: relative;
	z-index: 1000;
}

.toggle-btn {
	background: #333;
	color: white;
	border: none;
	padding: 10px;
	cursor: pointer;
}

/* Responsive behavior */
@media ( min-width : 768px) {
	.sidebar {
		transform: translateX(0);
		z-index: 1000;
	}
	.main-content {
		z-index: 1;
		margin-left: 250px;
	}
	.toggle-btn {
		display: none;
	}
	.toggle-btn-container.sidebar-active {
		left: 250px;
		z-index: 1001;
		transition: left 0.3s ease;
	}
	.toggle-btn-container {
		position: fixed;
		top: 15px;
		left: 15px;
		z-index: 1001;
		transition: left 0.3s ease;
	}
}

@media ( max-width : 1180px) {
	.slide-responsive {
		flex-direction: column;
	}
	#carouselExampleCaptions {
		height: 500px;
	}
}

@media ( max-width : 660px) {
	#carouselExampleCaptions {
		height: 600px;
		margin-bottom: 100px;
	}
	.carousel-text {
		width: 300px;
		padding: 10px;
	}
	.cards-container {
		position: relative;
		width: 270px;
		height: 800px;
		display: flex;
		justify-content: center;
		align-items: center;
	}
	.cards-container .product-card {
		position: absolute;
		box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
		border-radius: 10px;
		transition: transform 0.3s, box-shadow 0.3s;
		width: 9rem;
		height: auto;
	}
	.cards-container .product-card img {
		width: 100%;
		height: auto;
	}
	.cards-container .product-card .card-body {
		font-size: 0.9rem;
		padding: 0.5rem;
	}
	.cards-container .product-card:nth-child(1) {
		top: 0;
		left: 0px;
		z-index: 2;
	}
	.cards-container .product-card:nth-child(2) {
		top: 100px;
		left: 100px;
		z-index: 1;
	}
	.cards-container .product-card:hover {
		transform: scale(1.05);
		z-index: 3;
		box-shadow: 0 6px 10px rgba(0, 0, 0, 0.2);
	}
}
}
</style>
